model Ticket {
  id            String   @id @default(uuid())
  quantity      Int
  creation_time DateTime
  expiry_time   DateTime
  person_in     Int      @default(0)
  person_out    Int      @default(0)
}

model Order {
  order_id String @unique
  quantity Int
}
